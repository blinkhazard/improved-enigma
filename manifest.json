{
"manifest_version": 2,
    "name": "Snake people",
    "description": "Improve politician's name by adding their genuine quotes from a randomly selected list",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
